# Coeus Environment Variables
# Copy this file to .env and fill in your values

# Required: Anthropic API key for Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Tavily API key for web search capability
# Get one at https://tavily.com (free tier available)
TAVILY_API_KEY=your_tavily_api_key_here

# Neo4j connection (defaults work with docker-compose)
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=coeus_memory_2024
